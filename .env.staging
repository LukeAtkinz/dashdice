# Staging Environment Configuration
NEXT_PUBLIC_ENV=staging
NEXT_PUBLIC_SITE_URL=https://dashdice-dev.vercel.app
NEXT_PUBLIC_API_URL=https://dashdice-dev.vercel.app/api

# Firebase Staging (use separate project for safety)
NEXT_PUBLIC_FIREBASE_PROJECT_ID=dashdice-staging
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=dashdice-staging.firebaseapp.com
NEXT_PUBLIC_FIREBASE_DATABASE_URL=https://dashdice-staging-default-rtdb.firebaseio.com
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=dashdice-staging.appspot.com

# WebSocket for staging
NEXT_PUBLIC_WS_URL=wss://api-staging.dashdice.gg

# Debug mode for staging
NEXT_PUBLIC_DEBUG=true
NEXT_PUBLIC_SHOW_DEBUG_INFO=true