{
  "name": "DashDice",
  "integrations": {
    "capacitor": {
      "appId": "com.dashdice.app",
      "appName": "DashDice",
      "bundledWebRuntime": false,
      "webDir": "out"
    }
  },
  "type": "custom",
  "build": {
    "command": "set CAPACITOR_BUILD=true && npm run build",
    "outputDirectory": "out"
  },
  "serve": {
    "command": "npm run dev",
    "port": 3000
  }
}
